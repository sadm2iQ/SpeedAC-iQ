--------------------------------------------------------------------------*/
void ClearAnalogOuts(int ScaleNo)
{
 if(ScaleNo == 1)
  { 
   if(AOUTActive[AIO1]) aout(AIO1,0);
   if(AOUTActive[AIO3]) aout(AIO3,0);
  }
 
 if(ScaleNo == 2)
  { 
   if(AOUTActive[AIO2])  aout(AIO2,0);
   if(AOUTActive[AIO4])  aout(AIO4,0);
  } 
} 


